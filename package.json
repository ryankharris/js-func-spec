{
  "name": "js-func-spec",
  "version": "0.1.0",
  "description": "A Function Specification Lib",
  "main": "index.js",
  "scripts": {
    "build": "npm run do-prettier && npm run test",
    "do-prettier": "prettier --write ./index.js ./test/unit/*-spec.js",
    "test": "NODE_ENV=test jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryankharris/js-func-spec.git"
  },
  "keywords": [
    "javascript",
    "function",
    "specification",
    "contract",
    "type-enforcement",
    "type-coercion",
    "runtime"
  ],
  "author": "Ryan K Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryankharris/js-func-spec/issues"
  },
  "homepage": "https://github.com/ryankharris/js-func-spec#readme",
  "devDependencies": {
    "jasmine": "^2.8.0",
    "prettier": "^1.7.4"
  }
}
